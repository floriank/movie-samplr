<% content_for :header do %>
  <%= render partial: 'layouts/app_header' %>
<% end %>
<div class="edit-list">
  <h1><%= t('.rename') %></h1>
  <%= form_for(@list) do |f| %>
    <div class="form-group">
      <%= f.label :name, class: 'control-label sr-only' %>
      <%= f.text_field :name, class: 'form-control', placeholder: t('.name') %>
    </div>
    <%= f.submit t('.submit'), class: 'btn btn-default' %>
  <% end %>
</div>
